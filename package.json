{
  "name": "@juanibiapina/skulls",
  "version": "1.0.0",
  "description": "Simplified CLI for the open agent skills ecosystem",
  "type": "module",
  "bin": {
    "skulls": "./bin/cli.mjs"
  },
  "files": [
    "dist",
    "bin",
    "README.md",
    "ThirdPartyNoticeText.txt"
  ],
  "scripts": {
    "build": "obuild",
    "dev": "node src/cli.ts",
    "format": "prettier --write 'src/**/*.ts' 'tests/**/*.ts'",
    "format:check": "prettier --check 'src/**/*.ts' 'tests/**/*.ts'",
    "test": "vitest",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "cli",
    "agent-skills",
    "skills",
    "ai-agents",
    "skulls"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@clack/prompts": "^0.11.0",
    "@types/bun": "latest",
    "@types/node": "^22.10.0",
    "gray-matter": "^4.0.3",
    "obuild": "^0.4.22",
    "picocolors": "^1.1.1",
    "prettier": "^3.8.1",
    "simple-git": "^3.27.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.17"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "pnpm@10.17.1"
}
